define buttons => type {

	data 
		private x=integer,
		private y=integer
	public oncreate => {
		.reset
	}
	public reset => {
		.x=2
		.y=2
	}
	public move(movement::string) => {
		if(#movement==='U') => {
			.y+=1
		else(#movement==='D')
			.y-=1
		else(#movement==='R')
			.x+=1
		else(#movement==='L')
			.x-=1
		}
		.x<1 ? .x=1
		.x>3 ? .x=3
		.y<1 ? .y=1
		.y>3 ? .y=3
	}
	public decode => {
		local(button=.x)
		if(.y===1) => {
			#button+=6
		else(.y===2)
			#button+=3
		}
		return #button
	}
}

local(timer=date_msec)

local(buttons=buttons,
	code=string)

local(input='



')

#input=#input->split('\n')
iterate(#input) => {
	iterate(loop_value) => {
		#buttons->move(loop_value)
	}
	#code+=string(#buttons->decode)
}

'The code is ' + #code + ' calculated in ' (date_msec-#timer) + ' ms'

]
